#include <iostream>
#include <string>
#include <stack>
#define sonic ios::sync_with_stdio(false); cin.tie(nullptr);

using namespace std;
typedef long long ll;

string sol(string n) {
    stack<char> a;
    for (char i : n) {
        if (i == '(') {
            a.push('(');
        } else if (i == ')') {
            if (not a.empty() and a.top() == '(') {
                a.pop();
            } else {
                return "no";
            }
        } else if (i == '[') {
            a.push('[');
        } else if (i == ']') {
            if (not a.empty() and a.top() == '[') {
                a.pop();
            } else {
                return "no";
            }
        } else if (i == '{') {
            a.push('{');
        } else if (i == '}') {
            if (not a.empty() and a.top() == '{') {
                a.pop();
            } else {
                return "no";
            }
        }
    }
    if (a.size() != 0) {
        return "no";
    }
    return "yes";
}

int main() {
    sonic;
    string n; cin >> n;
    cout << sol(n) << "\n";
    return 0;
}
